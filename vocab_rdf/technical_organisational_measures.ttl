dpv:PseudoAnonymization a rdfs:Class ;
    rdfs:label "Pseudo-Anonymization"@en ;
    dct:created "2019-04-04"^^xsd:date ;
    dct:creator "Axel Polleres"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Mark Lizar"^^xsd:string,
        "Rob Brennan"^^xsd:string ;
    dct:date-accepted "2019-04-05"^^xsd:date ;
    dct:description "PseudoAnonmyization or 'pseudonymisation’ means the processing of personal data in such a manner that the personal data can no longer be attributed to a specific data subject without the use of additional information, provided that such additional information is kept separately and is subject to technical and organisational measures to ensure that the personal data are not attributed to an identified or identifiable natural person;"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_5/pnt_g/oj> ;
    rdfs:subClassOf dpv:PseudonymisationEncryption ;
    sw:term_status "accepted"@en .

