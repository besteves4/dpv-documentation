    <dct:date-accepted rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:date-accepted>
    <dct:description xml:lang="en">Indicates the processing is carried out at large scale in terms of size of data or individuals associated</dct:description>
  </rdf:Description>
