<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:sw="http://www.w3.org/2003/06/sw-vocab-status/ns#"
>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#LegalEntity">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:description xml:lang="en">A human or non-human that constitute as a legally defined entity</dct:description>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Legal Entity</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#Authority">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <dct:description xml:lang="en">An authority with the power to create or enforce laws, or determine their compliance.</dct:description>
    <rdfs:label xml:lang="en">Authority</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#LegalEntity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan Pandit</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasRepresentative">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <rdfs:label xml:lang="en">has representative</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J.Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:description xml:lang="en">Specifies representative of the legal entity</dct:description>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasIdentifier">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J.Pandit</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <rdfs:label xml:lang="en">has identifier</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <dct:description xml:lang="en">Specifies an identifier for the entity such as registeration number or official ID</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-25</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#Child">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#DataSubject"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">A 'child' is a natural legal person who is below a certain legal age depending on the legal jurisdiction.</dct:description>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment xml:lang="en">The legality of age defining a child varies by jurisdiction. In addition, 'child' is distinct from a 'minor'. For example, the legal age drinking alcohol can be 21, which makes a person of age 20 a 'minor' in this context. In other cases, 'minor' and 'child' are used interchangeably to refer to a person below some legally defined age.</rdfs:comment>
    <rdfs:label xml:lang="en">Child</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-25</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DataProcessor">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:description xml:lang="en">A ‘processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller.</dct:description>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
    <rdfs:label xml:lang="en">Data Processor</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_8/oj"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#Recipient"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#ThirdParty">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#Recipient"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_10/oj"/>
    <dct:description xml:lang="en">A ‘third party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and persons who, under the direct authority of the controller or processor, are authorised to process personal data;</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:label xml:lang="en">Third Party</rdfs:label>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasContact">
    <dct:description xml:lang="en">Specifies contact details of a legal entity such as phone  or email</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J.Pandit</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <rdfs:label xml:lang="en">has contact</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#Representative">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#LegalEntity"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <rdfs:label xml:lang="en">Representative</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:description xml:lang="en">A representative of a legal entity</dct:description>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_27/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#VulnerableDataSubject">
    <rdfs:label xml:lang="en">Vulnerable Data Subject</rdfs:label>
    <dct:description xml:lang="en">Data Subjects which should be considered 'vulnerable' and therefore would require additional measures and safeguards</dct:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#DataSubject"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan Pandit</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DataProtectionOfficer">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Data Protection Officer</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_37/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#LegalEntity"/>
    <dct:description xml:lang="en">An entity within or authorised by an organisation to monitor internal compliance, inform and advise on your data protection obligations and act as a contact point for data subjects and the supervisory authority.</dct:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasName">
    <dct:description xml:lang="en">Specifies name of a legal entity</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J.Pandit</dct:creator>
    <rdfs:label xml:lang="en">has name</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#hasAddress">
    <dct:description xml:lang="en">Specifies address of a legal entity such as street address or pin code</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <rdfs:label xml:lang="en">has address</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J.Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DataSubProcessor">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">Data Sub-Processor</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:description xml:lang="en">A 'sub-processor' is a processor engaged by another processor</dct:description>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#DataProcessor"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-25</dct:created>
    <rdfs:comment xml:lang="en">sub-processor' is a commonly used term similar to 'sub-contractor' and does not have a specific legal definition</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DataProtectionAuthority">
    <dct:description xml:lang="en">An authority tasked with overseeing legal compliance regarding privacy and data protection laws.</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan Pandit</dct:creator>
    <rdfs:label xml:lang="en">Data Protection Authority</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#Authority"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
</rdf:RDF>
