<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:sw="http://www.w3.org/2003/06/sw-vocab-status/ns#"
>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DataSubProcessor">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:description xml:lang="en">A processor engaged by another processor</dct:description>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_28/par_2/pnt_g/oj"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:label xml:lang="en">Data Sub-Processor</rdfs:label>
    <sw:term_status xml:lang="en">proposed</sw:term_status>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#DataProcessor"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DataProcessor">
    <sw:term_status xml:lang="en">approved</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_8/pnt_g/oj"/>
    <rdfs:label xml:lang="en">Data Processor</rdfs:label>
    <dct:date-accepted rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:date-accepted>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:description xml:lang="en">‘processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller;</dct:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#Recipient"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#ThirdParty">
    <dct:description xml:lang="en">‘third party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and persons who, under the direct authority of the controller or processor, are authorised to process personal data;</dct:description>
    <sw:term_status xml:lang="en">approved</sw:term_status>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#Recipient"/>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_10/pnt_g/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Third Party</rdfs:label>
    <dct:date-accepted rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:date-accepted>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#Child">
    <rdfs:comment xml:lang="en">A 'child' is distinct from a 'minor'. For example, the legal age for a 'minor' in most countries is 18, whereas a 'child' can be a minor below the age of 14.</rdfs:comment>
    <sw:term_status xml:lang="en">proposed</sw:term_status>
    <rdfs:label xml:lang="en">Child</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-06</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:description xml:lang="en">A 'child' is a natural legal person who is below a certain legal age depending on the legal jurisdiction.</dct:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#DataSubject"/>
  </rdf:Description>
</rdf:RDF>
